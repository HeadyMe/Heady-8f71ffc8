{
    "name": "heady-ai",
    "displayName": "Heady AI ‚Äî Intelligence Layer",
    "description": "AI assistant powered by the Heady Intelligence Layer. 5 branded models, 20 competing nodes, arena-validated responses. Chat, code, battle-validate, and create from your editor.",
    "version": "1.1.0",
    "publisher": "HeadyAI",
    "icon": "icon.png",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/heady-systems/heady-ai-vscode"
    },
    "homepage": "https://headyio.com",
    "bugs": {
        "url": "https://github.com/heady-systems/heady-ai-vscode/issues"
    },
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "AI",
        "Machine Learning",
        "Programming Languages",
        "Other"
    ],
    "keywords": [
        "ai",
        "heady",
        "code assistant",
        "mcp",
        "ai chat",
        "arena",
        "battle",
        "model"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "heady.chat",
                "title": "Heady: Chat",
                "category": "Heady"
            },
            {
                "command": "heady.selectModel",
                "title": "Heady: Select Model",
                "category": "Heady"
            },
            {
                "command": "heady.explain",
                "title": "Heady: Explain Selection",
                "category": "Heady"
            },
            {
                "command": "heady.refactor",
                "title": "Heady: Refactor Selection",
                "category": "Heady"
            },
            {
                "command": "heady.battle",
                "title": "Heady: Battle-Validate Selection",
                "category": "Heady"
            },
            {
                "command": "heady.swarm",
                "title": "Heady: Swarm Task",
                "category": "Heady"
            },
            {
                "command": "heady.creative",
                "title": "Heady: Creative Generation",
                "category": "Heady"
            },
            {
                "command": "heady.audit",
                "title": "Heady: Security Audit",
                "category": "Heady"
            },
            {
                "command": "heady.simulate",
                "title": "Heady: Monte Carlo Simulate",
                "category": "Heady"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "heady.explain",
                    "group": "heady@1",
                    "when": "editorHasSelection"
                },
                {
                    "command": "heady.refactor",
                    "group": "heady@2",
                    "when": "editorHasSelection"
                },
                {
                    "command": "heady.battle",
                    "group": "heady@3",
                    "when": "editorHasSelection"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "heady-sidebar",
                    "title": "Heady AI",
                    "icon": "icon.svg"
                }
            ]
        },
        "views": {
            "heady-sidebar": [
                {
                    "id": "heady.chatView",
                    "name": "Chat",
                    "type": "webview"
                }
            ]
        },
        "configuration": {
            "title": "Heady AI",
            "properties": {
                "heady.apiUrl": {
                    "type": "string",
                    "default": "https://manager.headysystems.com",
                    "description": "Heady API URL"
                },
                "heady.apiKey": {
                    "type": "string",
                    "default": "",
                    "description": "Heady API key (required for premium models)"
                },
                "heady.model": {
                    "type": "string",
                    "default": "heady-flash",
                    "enum": [
                        "heady-flash",
                        "heady-edge",
                        "heady-buddy",
                        "heady-reason",
                        "heady-battle-v1"
                    ],
                    "enumDescriptions": [
                        "‚ö° Fast & free ‚Äî top 3 fastest nodes (~1s)",
                        "üåê Edge inference ‚Äî sub-200ms latency",
                        "ü§ù Memory-aware companion (Pro)",
                        "üß† Extended thinking ‚Äî deep analysis (Premium)",
                        "üèÜ Full 20-node arena ‚Äî highest quality (Premium)"
                    ],
                    "description": "Select which Heady model to use for inference"
                }
            }
        },
        "keybindings": [
            {
                "command": "heady.chat",
                "key": "ctrl+shift+h",
                "mac": "cmd+shift+h"
            },
            {
                "command": "heady.selectModel",
                "key": "ctrl+shift+m",
                "mac": "cmd+shift+m"
            }
        ]
    },
    "scripts": {
        "package": "npx @vscode/vsce package --no-dependencies",
        "publish": "npx @vscode/vsce publish"
    },
    "devDependencies": {
        "@vscode/vsce": "^3.0.0"
    },
    "dependencies": {}
}