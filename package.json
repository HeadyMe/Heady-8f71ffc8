    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "glob": "^13.0.5",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "openai": "^4.71.1",
    "puppeteer": "^23.8.0",
    "uuid": "^10.0.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.1",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.13",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0"
