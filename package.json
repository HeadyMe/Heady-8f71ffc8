  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HeadySystems/Heady.git"
  },
  "config": {
    "headysoul_enabled": true,
    "socratic_mode": "exploratory",
    "zero_idle": true,
    "quantum_ready": true,
