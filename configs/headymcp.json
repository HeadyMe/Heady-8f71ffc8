  "modelRouting": {
    "defaultModel": "heady-brain",
    "policy": "heady_only",
    "headyPercentage": 100,
    "allowExternalOverride": false,
    "overrideRequires": "explicit_user_direction",
    "logAllModelSwitches": true,
    "fallbackChain": ["heady-brain", "heady-manager", "heady-soul"]
  },
  "googleAntigravity": {
    "enabled": true,
    "configPath": "~/.config/google/antigravity/mcp.json",
    "routingPolicy": "heady_only",
    "headyPercentage": 100,
    "mcpServer": "heady-local"
  },
  "interNodeComms": {
    "defaultTimeoutMs": 10000,
    "healthCheckIntervalMs": 30000,
    "retryPolicy": {
      "maxRetries": 3,
      "backoffBaseMs": 500,
      "backoffMultiplier": 2,
      "retryableErrors": ["ECONNREFUSED", "ETIMEDOUT", "429", "503", "UNAVAILABLE"]
    },
    "circuitBreaker": {
      "enabled": true,
      "failureThreshold": 5,
      "recoveryTimeMs": 60000,
      "halfOpenMaxRequests": 2
    },
    "connectionPool": {
      "maxSockets": 50,
      "maxFreeSockets": 10,
      "keepAliveMsecs": 30000
    },
    "authorizedKeys": [
      "${HEADY_API_KEY}",
      "${HEADY_API_KEY_002}",
      "${HEADY_API_KEY_003}",
      "${HEADY_API_KEY_004}",
